{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/all"
    ],
    "plugins": [
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": ["error", "as-needed"]
    }
}
